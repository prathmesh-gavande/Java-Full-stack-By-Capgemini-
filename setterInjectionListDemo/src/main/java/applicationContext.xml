<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->
	<beans>
		<bean id="a1" class="com.setterInjectionListDemo.entity.Question">
			<property name="id" value="1"></property>
			<property name="name" value="What is hibernate"></property>
			<property name="answer">
				<list>
					<value>It is orm tool</value>
					<value>It is java framework</value>
					<value>It is JPa implementation</value>
				</list>
			</property>
		</bean>
		<bean id="a2" class="com.setterInjectionListDemo.entity.Question">
			<property name="id" value="2"></property>
			<property name="name" value="what is your name"></property>
			<property name="answer">
				<list>
					<value>Prathmesh</value>
					<value>Gavande</value>
				</list>
			</property>
		</bean>
	</beans>

</beans>
